                            {
                                "from": {
                                    "key_code": "n",
                                    "modifiers": {
                                        "mandatory": [
                                            "right_command"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "open /Applications/NetNewsWire.app/"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Use right_command+j/k to switch windows of the foreground application .",
                        "manipulators": [
                          {
                            "type": "basic",
                            "from": {
                              "key_code": "k",
                              "modifiers": {
                                "mandatory": [
                                  "right_command"
                                ]
                              }
                            },
                            "to": [
                              {
                                "key_code": "grave_accent_and_tilde",
                                "modifiers": [
                                  "left_command"
                                ]
                              }
                            ]
                          },
                          {
                            "type": "basic",
                            "from": {
                              "key_code": "j",
                              "modifiers": {
                                "mandatory": [
                                  "right_command"
                                ]
                              }
                            },
                            "to": [
                              {
                                "key_code": "grave_accent_and_tilde",
                                "modifiers": [
                                  "left_command",
                                  "left_shift"
                                ]
                              }
                            ]
                          }
                        ]
                      }                  
                ]
